<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial=scale=1.0">
    <title> All Class Information | Study.io</title>
    <style>
      .quiz {
        background-color: aquamarine
      }
    </style>
  </head>
  <body>
    <% for (const class of classes) { %>

      <div class="class">
        <p class="className"><%= class.className %></p>

        <p class="classTimes"><%= class.classTimes %></p>

        <p class="classTextbook"><%= class.classTextbook %></p>

        <p class="courseDescription"><% class.courseDescription %></p>

        <p class="professorEmail"><% class.professorEmail %></p>

        <p class="officeHours"><% class.officeHours %></p>

      </div>
      <% } %>
  </body>
</html>
