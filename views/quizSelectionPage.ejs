<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>All Quizzes | Study.io</title>
  </head>
  <body>
    <!-- Wrap in form, include button -->
    <form method="GET" action="/quizzes/takeQuiz">
      <div>
        <label for="quiz-select"> Select Your Quiz</label>

        <select name="quizId" id="quiz-select">
          <% for(const quiz of quizzes) { %>

           <div class="quiz">
              <option value="<%= quiz.quizId %>"><%= quiz.setName %></option>
            </div>
          <% } %>
        </select>
      </div>

      <button id="quizSelectionBtn"> Select This Quiz</button>
    </form>
  </body>
</html>
